      "source": [
        "n, m = map(int, input().split())\n",
        "\n",
        "a = []\n",
        "\n",
        "for i in range(1, n+1):\n",
        "  if n % i == 0:\n",
        "    a.append(i)\n",
        "  else:\n",
        "    continue\n",
        "\n",
        "if m-1 < len(a):\n",
        "  print(a[m-1])\n",
        "else:\n",
        "  print(0)"
      ],
