        "a = int(input())\n",
        "\n",
        "for _ in range (a):\n",
        "  n = int(input())\n",
        "  for i in range(2, n+1):\n",
        "    c = 0\n",
        "    if n % i == 0:\n",
        "      while n % i == 0:\n",
        "        n = n//i\n",
        "        c += 1\n",
        "      print(\"%d %d\" %(i, c))\n",
        "    elif a == 1:\n",
        "      break"
