      "source": [
        "def gcd(a, b):\n",
        "    return gcd(b, a % b) if b else a\n",
        "def lcm(a, b):\n",
        "    return a * b // gcd(a, b)\n",
        "\n",
        "t = int(input())\n",
        "for i in range(t):\n",
        "    a, b = map(int, input().split())\n",
        "    print(lcm(a, b))"
      ],
